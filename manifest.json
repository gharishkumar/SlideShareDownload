{
   "author": "Harish Kumar",
   "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
    "browser_action": {
      "default_title": "SlideShare Downloader"
    },
   "description": "Download slide from slide share in pdf, ppt",
   "icons": {
      "128": "icon.png",
      "64": "icon.png"
   },
   "manifest_version": 2,
   "name": "SlideShare Downloader",
   "permissions": [ "activeTab" , "https://www.slideshare.net/*", "https://image.slidesharecdn.com/*", "*://*/*"],
   "version": "2.7",
   "web_accessible_resources": [ "*.js"]
}
